				pi.Name = "pi"
				pi.Parent = TabFrame
				pi.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
				pi.BorderSizePixel = 0
				pi.Position = UDim2.new(0.0107238609, 0, 0.078125, 0)
				pi.Size = UDim2.new(0, 370, 0, 27)

