			labelLayout2.FillDirection = Enum.FillDirection.Horizontal
			labelLayout2.HorizontalAlignment = Enum.HorizontalAlignment.Center
			labelLayout2.SortOrder = Enum.SortOrder.LayoutOrder
			labelLayout2.VerticalAlignment = Enum.VerticalAlignment.Center

			labelDesign2.Name = "labelDesign2"
